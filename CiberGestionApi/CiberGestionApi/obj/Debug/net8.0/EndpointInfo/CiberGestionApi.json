            "nullable": true
          },
          "vendedorId": {
            "type": "integer",
            "format": "int32"
          },
          "clienteId": {
            "type": "integer",
            "format": "int32"
          },
          "fecha": {
            "type": "string",
            "format": "date-time"
          },
          "moneda": {
            "type": "string",
            "nullable": true
          },
          "detalles": {
            "type": "array",
